extends ../layouts/base

block content

    #main-content.relative.w-full.h-full.overflow-y-auto.bg-gray-50.p-3.pt-5(class='lg:ml-64 dark:bg-gray-900')
      .p-4.bg-white.block.items-center.justify-between.border-b.border-gray-200(class='sm:flex lg:mt-1.5 dark:bg-gray-800 dark:border-gray-700')
        .w-full.mb-1
          .mb-4
            nav.flex.mb-5(aria-label='Breadcrumb')
              ol.inline-flex.items-center.space-x-1.text-sm.font-medium(class='md:space-x-2')
                li.inline-flex.items-center
                  a.inline-flex.items-center.text-gray-700(href='#', class='hover:text-primary-600 dark:text-gray-300 dark:hover:text-white')
                    svg.w-5.h-5(class='mr-2.5', fill='currentColor', viewBox='0 0 20 20', xmlns='http://www.w3.org/2000/svg')
                      path(d='M10.707 2.293a1 1 0 00-1.414 0l-7 7a1 1 0 001.414 1.414L4 10.414V17a1 1 0 001 1h2a1 1 0 001-1v-2a1 1 0 011-1h2a1 1 0 011 1v2a1 1 0 001 1h2a1 1 0 001-1v-6.586l.293.293a1 1 0 001.414-1.414l-7-7z')
                                                  Home
                li
                  .flex.items-center
                    svg.w-6.h-6.text-gray-400(fill='currentColor', viewBox='0 0 20 20', xmlns='http://www.w3.org/2000/svg')
                      path(fill-rule='evenodd', d='M7.293 14.707a1 1 0 010-1.414L10.586 10 7.293 6.707a1 1 0 011.414-1.414l4 4a1 1 0 010 1.414l-4 4a1 1 0 01-1.414 0z', clip-rule='evenodd')
                    a.ml-1.text-gray-700(href='#', class='hover:text-primary-600 md:ml-2 dark:text-gray-300 dark:hover:text-white') E-commerce
                li
                  .flex.items-center
                    svg.w-6.h-6.text-gray-400(fill='currentColor', viewBox='0 0 20 20', xmlns='http://www.w3.org/2000/svg')
                      path(fill-rule='evenodd', d='M7.293 14.707a1 1 0 010-1.414L10.586 10 7.293 6.707a1 1 0 011.414-1.414l4 4a1 1 0 010 1.414l-4 4a1 1 0 01-1.414 0z', clip-rule='evenodd')
                    span.ml-1.text-gray-400(class='md:ml-2 dark:text-gray-500', aria-current='page') Products
            h1.text-xl.font-semibold.text-gray-900(class='sm:text-2xl dark:text-white') Notebooks
          .items-center.justify-between.block(class='sm:flex md:divide-x md:divide-gray-100 dark:divide-gray-700')
            .flex.items-center.mb-4(class='sm:mb-0')
              form(class='sm:pr-3', action='#', method='GET')
                label.sr-only(for='products-search') Search
                .relative.w-48.mt-1(class='sm:w-64 xl:w-96')
                  input#products-search.bg-gray-50.border.border-gray-300.text-gray-900.rounded-lg.block.w-full(type='text', name='email', class='sm:text-sm focus:ring-primary-500 focus:border-primary-500 p-2.5 dark:bg-gray-700 dark:border-gray-600 dark:placeholder-gray-400 dark:text-white dark:focus:ring-primary-500 dark:focus:border-primary-500', placeholder='Search for products')
              .flex.items-center.w-full(class='sm:justify-end')
                .flex.pl-2.space-x-1
                  a.inline-flex.justify-center.p-1.text-gray-500.rounded.cursor-pointer(href='#', class='hover:text-gray-900 hover:bg-gray-100 dark:text-gray-400 dark:hover:bg-gray-700 dark:hover:text-white')
                    svg.w-6.h-6(fill='currentColor', viewBox='0 0 20 20', xmlns='http://www.w3.org/2000/svg')
                      path(fill-rule='evenodd', d='M11.49 3.17c-.38-1.56-2.6-1.56-2.98 0a1.532 1.532 0 01-2.286.948c-1.372-.836-2.942.734-2.106 2.106.54.886.061 2.042-.947 2.287-1.561.379-1.561 2.6 0 2.978a1.532 1.532 0 01.947 2.287c-.836 1.372.734 2.942 2.106 2.106a1.532 1.532 0 012.287.947c.379 1.561 2.6 1.561 2.978 0a1.533 1.533 0 012.287-.947c1.372.836 2.942-.734 2.106-2.106a1.533 1.533 0 01.947-2.287c1.561-.379 1.561-2.6 0-2.978a1.532 1.532 0 01-.947-2.287c.836-1.372-.734-2.942-2.106-2.106a1.532 1.532 0 01-2.287-.947zM10 13a3 3 0 100-6 3 3 0 000 6z', clip-rule='evenodd')
                  a.inline-flex.justify-center.p-1.text-gray-500.rounded.cursor-pointer(href='#', class='hover:text-gray-900 hover:bg-gray-100 dark:text-gray-400 dark:hover:bg-gray-700 dark:hover:text-white')
                    svg.w-6.h-6(fill='currentColor', viewBox='0 0 20 20', xmlns='http://www.w3.org/2000/svg')
                      path(fill-rule='evenodd', d='M9 2a1 1 0 00-.894.553L7.382 4H4a1 1 0 000 2v10a2 2 0 002 2h8a2 2 0 002-2V6a1 1 0 100-2h-3.382l-.724-1.447A1 1 0 0011 2H9zM7 8a1 1 0 012 0v6a1 1 0 11-2 0V8zm5-1a1 1 0 00-1 1v6a1 1 0 102 0V8a1 1 0 00-1-1z', clip-rule='evenodd')
                  a.inline-flex.justify-center.p-1.text-gray-500.rounded.cursor-pointer(href='#', class='hover:text-gray-900 hover:bg-gray-100 dark:text-gray-400 dark:hover:bg-gray-700 dark:hover:text-white')
                    svg.w-6.h-6(fill='currentColor', viewBox='0 0 20 20', xmlns='http://www.w3.org/2000/svg')
                      path(fill-rule='evenodd', d='M18 10a8 8 0 11-16 0 8 8 0 0116 0zm-7 4a1 1 0 11-2 0 1 1 0 012 0zm-1-9a1 1 0 00-1 1v4a1 1 0 102 0V6a1 1 0 00-1-1z', clip-rule='evenodd')
                  a.inline-flex.justify-center.p-1.text-gray-500.rounded.cursor-pointer(href='#', class='hover:text-gray-900 hover:bg-gray-100 dark:text-gray-400 dark:hover:bg-gray-700 dark:hover:text-white')
                    svg.w-6.h-6(fill='currentColor', viewBox='0 0 20 20', xmlns='http://www.w3.org/2000/svg')
                      path(d='M10 6a2 2 0 110-4 2 2 0 010 4zM10 12a2 2 0 110-4 2 2 0 010 4zM10 18a2 2 0 110-4 2 2 0 010 4z')

      .flex.flex-col
        .overflow-x-auto
          .inline-block.min-w-full.align-middle
            .overflow-hidden.shadow
              table#tableContainer.min-w-full.divide-y.divide-gray-200.table-fixed(class='dark:divide-gray-600')
                thead.bg-gray-100(class='dark:bg-gray-700')
                  tr
                    th.p-4(scope='col')
                      .flex.items-center
                        input#checkbox-all.w-4.h-4.border-gray-300.rounded.bg-gray-50(aria-describedby='checkbox-1', type='checkbox', class='focus:ring-3 focus:ring-primary-300 dark:focus:ring-primary-600 dark:ring-offset-gray-800 dark:bg-gray-700 dark:border-gray-600')
                        label.sr-only(for='checkbox-all') checkbox
                    th.p-4.text-xs.font-medium.text-left.text-gray-500.uppercase(scope='col', class='dark:text-gray-400')
                      | Title
                    th.p-4.text-xs.font-medium.text-left.text-gray-500.uppercase(scope='col', class='dark:text-gray-400')
                      | Description
                    th.p-4.text-xs.font-medium.text-left.text-gray-500.uppercase(scope='col', class='dark:text-gray-400')
                      | Updated At
                    th.p-4.text-xs.font-medium.text-left.text-gray-500.uppercase(scope='col', class='dark:text-gray-400')
                      | Actions
                tbody#tableContent.bg-white.divide-y.divide-gray-200(class='dark:bg-gray-800 dark:divide-gray-700')
                  tr(class='hover:bg-gray-100 dark:hover:bg-gray-700')
                    td.w-4.p-4
                      .flex.items-center
                        input#checkbox-194556.w-4.h-4.border-gray-300.rounded.bg-gray-50(aria-describedby='checkbox-1', type='checkbox', class='focus:ring-3 focus:ring-primary-300 dark:focus:ring-primary-600 dark:ring-offset-gray-800 dark:bg-gray-700 dark:border-gray-600')
                        label.sr-only(for='checkbox-194556') checkbox
                    td.p-4.text-sm.font-normal.text-gray-500.whitespace-nowrap(class='dark:text-gray-400')
                      .text-base.font-semibold.text-gray-900(class='dark:text-white')
                        | Education
                        | Dashboard
                      .text-sm.font-normal.text-gray-500(class='dark:text-gray-400')
                        | 11/27/2024 (created at)
                    td.p-4.text-base.font-medium.text-gray-900.whitespace-nowrap(class='dark:text-white')
                      | Start developing....
                    td.p-4.text-base.font-medium.text-gray-900.whitespace-nowrap(class='dark:text-white')
                      | 11/27/2024 (updated at)
                    td.p-4.space-x-2.whitespace-nowrap
                      button#updateProductButton.inline-flex.items-center.px-3.py-2.text-sm.font-medium.text-center.text-white.rounded-lg.bg-primary-700(type='button', aria-controls='drawer-update-product-default', class='hover:bg-primary-800 focus:ring-4 focus:ring-primary-300 dark:bg-primary-600 dark:hover:bg-primary-700 dark:focus:ring-primary-800')
                        svg.w-4.h-4.mr-2(fill='currentColor', viewBox='0 0 20 20', xmlns='http://www.w3.org/2000/svg')
                          path(d='M17.414 2.586a2 2 0 00-2.828 0L7 10.172V13h2.828l7.586-7.586a2 2 0 000-2.828z')
                          path(fill-rule='evenodd', d='M2 6a2 2 0 012-2h4a1 1 0 010 2H4v10h10v-4a1 1 0 112 0v4a2 2 0 01-2 2H4a2 2 0 01-2-2V6z', clip-rule='evenodd')
                        | Edit
                      button#deleteProductButton.inline-flex.items-center.px-3.py-2.text-sm.font-medium.text-center.text-white.bg-red-700.rounded-lg(type='button', aria-controls='drawer-delete-product-default', class='hover:bg-red-800 focus:ring-4 focus:ring-red-300 dark:focus:ring-red-900')
                        svg.w-4.h-4.mr-2(fill='currentColor', viewBox='0 0 20 20', xmlns='http://www.w3.org/2000/svg')
                          path(fill-rule='evenodd', d='M9 2a1 1 0 00-.894.553L7.382 4H4a1 1 0 000 2v10a2 2 0 002 2h8a2 2 0 002-2V6a1 1 0 100-2h-3.382l-.724-1.447A1 1 0 0011 2H9zM7 8a1 1 0 012 0v6a1 1 0 11-2 0V8zm5-1a1 1 0 00-1 1v6a1 1 0 102 0V8a1 1 0 00-1-1z', clip-rule='evenodd')
                        | Delete

    script.
      const notesID = !{JSON.stringify(notesID)}
      const arrayImages = !{JSON.stringify(arrayImages)};
      const titles = !{JSON.stringify(titles)};
      const descs = !{JSON.stringify(descs)};
      const updatedAt = !{JSON.stringify(updatedAt)};
      const createdAt = !{JSON.stringify(createdAt)};
    script(src='/js/notes/table.note.js')